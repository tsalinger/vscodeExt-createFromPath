{
    "name": "foldercreator",
    "displayName": "FolderCreator",
    "description": "Create multiple folders at once",
    "version": "0.0.1",
    "publisher": "publisher",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:main.createNewFolders"
    ],
    "main": "./out/main",
    "contributes": {
        "menus": {
            "explorer/context": [
                {
                    "command": "main.createNewFolders",
                    "group": "2_workspace"
                }
            ]
        },
        "commands": [
            {
                "command": "main.createNewFolders",
                "title": "New Folders From Path"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "is-valid-path": "^0.1.1"
    }
}
